{
  "name": "@sklep/types",
  "version": "1.0.0-alpha.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate-api-types && yarn restart",
    "restart": "wait-on http://api.sklep.localhost:3002 -s 1 --reverse && yarn generate-api-types && yarn restart",
    "generate-api-types": "wait-on http://api.sklep.localhost:3002 -s 1 --interval 5000 && yarn swagger-to-ts http://api.sklep.localhost:3002/swagger.json --output types.ts --prettier-config ../../.prettierrc"
  },
  "dependencies": {
    "@manifoldco/swagger-to-ts": "github:mmiszy/swagger-to-ts#fae0dceec79e17caa8d6bb013486d149b9927c5f"
  },
  "devDependencies": {
    "chokidar-cli": "2.1.0",
    "wait-on": "5.2.0"
  }
}
